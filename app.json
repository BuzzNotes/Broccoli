{
  "expo": {
    "name": "Broccoli",
    "slug": "Broccoli",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "broccoli",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sixfortylabs.broccoli",
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1079081190424-bo66njc6lellu79jdgnmeog14p3c904s"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sixfortylabs.broccoli",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-router",
      "expo-font",
      "expo-video",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": false,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "de979687-ff43-4b3b-89b0-d02b144c9141"
      }
    },
    "owner": "austincotter"
  }
}
